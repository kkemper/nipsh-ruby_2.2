<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Current Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi organization</th>
      <th>Bsdi department</th>
      <th>Bsdi service</th>
      <th>Bsdi job title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_current_jobs.each do |bsdi_current_job| %>
      <tr>
        <td><%= bsdi_current_job.bsdi_report_id %></td>
        <td><%= bsdi_current_job.bsdi_organization %></td>
        <td><%= bsdi_current_job.bsdi_department %></td>
        <td><%= bsdi_current_job.bsdi_service %></td>
        <td><%= bsdi_current_job.bsdi_job_title %></td>
        <td><%= link_to 'Show', bsdi_current_job %></td>
        <td><%= link_to 'Edit', edit_bsdi_current_job_path(bsdi_current_job) %></td>
        <td><%= link_to 'Destroy', bsdi_current_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi current job', new_bsdi_current_job_path %>
