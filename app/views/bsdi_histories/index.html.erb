<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Histories</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi family stroke</th>
      <th>Bsdi family cancer</th>
      <th>Bsdi family diabetes</th>
      <th>Bsdi family cholesterol</th>
      <th>Bsdi family hypertension</th>
      <th>Bsdi family heart only</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_histories.each do |bsdi_history| %>
      <tr>
        <td><%= bsdi_history.bsdi_report_id %></td>
        <td><%= bsdi_history.bsdi_family_stroke %></td>
        <td><%= bsdi_history.bsdi_family_cancer %></td>
        <td><%= bsdi_history.bsdi_family_diabetes %></td>
        <td><%= bsdi_history.bsdi_family_cholesterol %></td>
        <td><%= bsdi_history.bsdi_family_hypertension %></td>
        <td><%= bsdi_history.bsdi_family_heart_only %></td>
        <td><%= link_to 'Show', bsdi_history %></td>
        <td><%= link_to 'Edit', edit_bsdi_history_path(bsdi_history) %></td>
        <td><%= link_to 'Destroy', bsdi_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

