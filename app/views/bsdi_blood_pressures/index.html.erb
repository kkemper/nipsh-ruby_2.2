<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Blood Pressures</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi systolic</th>
      <th>Bsdi diastolic</th>
      <th>Bsdi bp medicine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_blood_pressures.each do |bsdi_blood_pressure| %>
      <tr>
        <td><%= bsdi_blood_pressure.bsdi_report_id %></td>
        <td><%= bsdi_blood_pressure.bsdi_systolic %></td>
        <td><%= bsdi_blood_pressure.bsdi_diastolic %></td>
        <td><%= bsdi_blood_pressure.bsdi_bp_medicine %></td>
        <td><%= link_to 'Show', bsdi_blood_pressure %></td>
        <td><%= link_to 'Edit', edit_bsdi_blood_pressure_path(bsdi_blood_pressure) %></td>
        <td><%= link_to 'Destroy', bsdi_blood_pressure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi blood pressure', new_bsdi_blood_pressure_path %>
