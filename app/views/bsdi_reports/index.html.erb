<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Reports</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi patient</th>
      <th>Bsdi date</th>
      <th>Bsdi newest</th>
      <th>Bsdi valid year</th>
      <th>Bsdi user defined flag</th>
      <th>Bsdi staff entry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_reports.each do |bsdi_report| %>
      <tr>
        <td><%= bsdi_report.bsdi_patient_id %></td>
        <td><%= bsdi_report.bsdi_date %></td>
        <td><%= bsdi_report.bsdi_newest %></td>
        <td><%= bsdi_report.bsdi_valid_year %></td>
        <td><%= bsdi_report.bsdi_user_defined_flag %></td>
        <td><%= bsdi_report.bsdi_staff_entry %></td>
        <td><%= link_to 'Show', bsdi_report %></td>
        <td><%= link_to 'Edit', edit_bsdi_report_path(bsdi_report) %></td>
        <td><%= link_to 'Destroy', bsdi_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi report', new_bsdi_report_path %>
