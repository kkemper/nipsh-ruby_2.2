<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Activities</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi rfc activity</th>
      <th>Bsdi active barrier time</th>
      <th>Bsdi active barrier people</th>
      <th>Bsdi active barrier unfamiliar</th>
      <th>Bsdi active barrier resources</th>
      <th>Bsdi active barrier unpleasant</th>
      <th>Bsdi active barrier medical worry</th>
      <th>Bsdi active barrier intimidating</th>
      <th>Bsdi active barrier embarrassed</th>
      <th>Bsdi active barrier other</th>
      <th>Bsdi active barrier injury</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_activities.each do |bsdi_activity| %>
      <tr>
        <td><%= bsdi_activity.bsdi_report_id %></td>
        <td><%= bsdi_activity.bsdi_rfc_activity %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_time %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_people %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_unfamiliar %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_resources %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_unpleasant %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_medical_worry %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_intimidating %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_embarrassed %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_other %></td>
        <td><%= bsdi_activity.bsdi_active_barrier_injury %></td>
        <td><%= link_to 'Show', bsdi_activity %></td>
        <td><%= link_to 'Edit', edit_bsdi_activity_path(bsdi_activity) %></td>
        <td><%= link_to 'Destroy', bsdi_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

