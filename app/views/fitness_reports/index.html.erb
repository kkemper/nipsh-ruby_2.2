<p id="notice"><%= notice %></p>

<h1>Listing Fitness Reports</h1>

<table>
  <thead>
    <tr>
      <th>Fitness report</th>
      <th>Fitness patient</th>
      <th>Fitness test date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fitness_reports.each do |fitness_report| %>
      <tr>
        <td><%= fitness_report.fitness_report_id %></td>
        <td><%= fitness_report.fitness_patient_id %></td>
        <td><%= fitness_report.fitness_test_date %></td>
        <td><%= link_to 'Show', fitness_report %></td>
        <td><%= link_to 'Edit', edit_fitness_report_path(fitness_report) %></td>
        <td><%= link_to 'Destroy', fitness_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fitness report', new_fitness_report_path %>
