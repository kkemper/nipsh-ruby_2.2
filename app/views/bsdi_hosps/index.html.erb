<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Hosps</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi hospitalization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_hosps.each do |bsdi_hosp| %>
      <tr>
        <td><%= bsdi_hosp.bsdi_report_id %></td>
        <td><%= bsdi_hosp.bsdi_hospitalization %></td>
        <td><%= link_to 'Show', bsdi_hosp %></td>
        <td><%= link_to 'Edit', edit_bsdi_hosp_path(bsdi_hosp) %></td>
        <td><%= link_to 'Destroy', bsdi_hosp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi hosp', new_bsdi_hosp_path %>
