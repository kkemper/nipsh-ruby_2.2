<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Smokings</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi smoking status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_smokings.each do |bsdi_smoking| %>
      <tr>
        <td><%= bsdi_smoking.bsdi_report_id %></td>
        <td><%= bsdi_smoking.bsdi_smoking_status %></td>
        <td><%= link_to 'Show', bsdi_smoking %></td>
        <td><%= link_to 'Edit', edit_bsdi_smoking_path(bsdi_smoking) %></td>
        <td><%= link_to 'Destroy', bsdi_smoking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi smoking', new_bsdi_smoking_path %>
