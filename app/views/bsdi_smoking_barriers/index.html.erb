<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Smoking Barriers</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi rfc smoking</th>
      <th>Bsdi smoke barrier knowledge</th>
      <th>Bsdi smoke barrier temptation</th>
      <th>Bsdi smoke barrier friends</th>
      <th>Bsdi smoke barrier weight</th>
      <th>Bsdi smoke barrier stress</th>
      <th>Bsdi smoke barrier other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_smoking_barriers.each do |bsdi_smoking_barrier| %>
      <tr>
        <td><%= bsdi_smoking_barrier.bsdi_report_id %></td>
        <td><%= bsdi_smoking_barrier.bsdi_rfc_smoking %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_knowledge %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_temptation %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_friends %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_weight %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_stress %></td>
        <td><%= bsdi_smoking_barrier.bsdi_smoke_barrier_other %></td>
        <td><%= link_to 'Show', bsdi_smoking_barrier %></td>
        <td><%= link_to 'Edit', edit_bsdi_smoking_barrier_path(bsdi_smoking_barrier) %></td>
        <td><%= link_to 'Destroy', bsdi_smoking_barrier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

