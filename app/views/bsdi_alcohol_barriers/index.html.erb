<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Alcohol Barriers</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi rfc alcohol</th>
      <th>Bsdi alcohol barrier knowledge</th>
      <th>Bsdi alcohol barrier temptation</th>
      <th>Bsdi alcohol barrier friends</th>
      <th>Bsdi alcohol barrier perception</th>
      <th>Bsdi alcohol barrier stress</th>
      <th>Bsdi alcohol barrier other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_alcohol_barriers.each do |bsdi_alcohol_barrier| %>
      <tr>
        <td><%= bsdi_alcohol_barrier.bsdi_report_id %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_rfc_alcohol %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_knowledge %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_temptation %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_friends %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_perception %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_stress %></td>
        <td><%= bsdi_alcohol_barrier.bsdi_alcohol_barrier_other %></td>
        <td><%= link_to 'Show', bsdi_alcohol_barrier %></td>
        <td><%= link_to 'Edit', edit_bsdi_alcohol_barrier_path(bsdi_alcohol_barrier) %></td>
        <td><%= link_to 'Destroy', bsdi_alcohol_barrier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

