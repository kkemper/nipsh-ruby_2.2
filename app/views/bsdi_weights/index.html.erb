<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Weights</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Rfc weight</th>
      <th>Bsdi weight barrier knowledge</th>
      <th>Bsdi weight barrier access</th>
      <th>Bsdi weight barrier social</th>
      <th>Bsdi weight barrier exercise</th>
      <th>Bsdi weight barrier stress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_weights.each do |bsdi_weight| %>
      <tr>
        <td><%= bsdi_weight.bsdi_report_id %></td>
        <td><%= bsdi_weight.rfc_weight %></td>
        <td><%= bsdi_weight.bsdi_weight_barrier_knowledge %></td>
        <td><%= bsdi_weight.bsdi_weight_barrier_access %></td>
        <td><%= bsdi_weight.bsdi_weight_barrier_social %></td>
        <td><%= bsdi_weight.bsdi_weight_barrier_exercise %></td>
        <td><%= bsdi_weight.bsdi_weight_barrier_stress %></td>
        <td><%= link_to 'Show', bsdi_weight %></td>
        <td><%= link_to 'Edit', edit_bsdi_weight_path(bsdi_weight) %></td>
        <td><%= link_to 'Destroy', bsdi_weight, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi weight', new_bsdi_weight_path %>
