<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Measurements</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi weight</th>
      <th>Bsdi height</th>
      <th>Bsdi waist</th>
      <th>Bsdi waist risk</th>
      <th>Bsdi hips</th>
      <th>Bsdi weight belief</th>
      <th>Bsdi bmi</th>
      <th>Bsdi bmi category</th>
      <th>Bsdi high risk bmi</th>
      <th>Bsdi hypertrophic</th>
      <th>Bsdi disabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_measurements.each do |bsdi_measurement| %>
      <tr>
        <td><%= bsdi_measurement.bsdi_weight %></td>
        <td><%= bsdi_measurement.bsdi_height %></td>
        <td><%= bsdi_measurement.bsdi_waist %></td>
        <td><%= bsdi_measurement.bsdi_waist_risk %></td>
        <td><%= bsdi_measurement.bsdi_hips %></td>
        <td><%= bsdi_measurement.bsdi_weight_belief %></td>
        <td><%= bsdi_measurement.bsdi_bmi %></td>
        <td><%= bsdi_measurement.bsdi_bmi_category %></td>
        <td><%= bsdi_measurement.bsdi_high_risk_bmi %></td>
        <td><%= bsdi_measurement.bsdi_hypertrophic %></td>
        <td><%= bsdi_measurement.bsdi_disabled %></td>
        <td><%= link_to 'Show', bsdi_measurement %></td>
        <td><%= link_to 'Edit', edit_bsdi_measurement_path(bsdi_measurement) %></td>
        <td><%= link_to 'Destroy', bsdi_measurement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bsdi measurement', new_bsdi_measurement_path %>
