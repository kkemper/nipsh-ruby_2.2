<p id="notice"><%= notice %></p>

<h1>Listing Bsdi Blood Chemistries</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi bchem date</th>
      <th>Bsdi cholesterol</th>
      <th>Bsdi high cholesterol</th>
      <th>Bsdi cholesterol meds</th>
      <th>Bsdi hdl</th>
      <th>Bsdi hdl status</th>
      <th>Bsdi ldl</th>
      <th>Bsdi high ldl</th>
      <th>Bsdi triglycerides</th>
      <th>Bsdi high triglyceride</th>
      <th>Bsdi glucose</th>
      <th>Bsdi pre diabetes</th>
      <th>Bsdi a1c</th>
      <th>Bsdi blood chem risk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_blood_chemistries.each do |bsdi_blood_chemistry| %>
      <tr>
        <td><%= bsdi_blood_chemistry.bsdi_report_id %></td>
        <td><%= bsdi_blood_chemistry.bsdi_bchem_date %></td>
        <td><%= bsdi_blood_chemistry.bsdi_cholesterol %></td>
        <td><%= bsdi_blood_chemistry.bsdi_high_cholesterol %></td>
        <td><%= bsdi_blood_chemistry.bsdi_cholesterol_meds %></td>
        <td><%= bsdi_blood_chemistry.bsdi_hdl %></td>
        <td><%= bsdi_blood_chemistry.bsdi_hdl_status %></td>
        <td><%= bsdi_blood_chemistry.bsdi_ldl %></td>
        <td><%= bsdi_blood_chemistry.bsdi_high_ldl %></td>
        <td><%= bsdi_blood_chemistry.bsdi_triglycerides %></td>
        <td><%= bsdi_blood_chemistry.bsdi_high_triglyceride %></td>
        <td><%= bsdi_blood_chemistry.bsdi_glucose %></td>
        <td><%= bsdi_blood_chemistry.bsdi_pre_diabetes %></td>
        <td><%= bsdi_blood_chemistry.bsdi_a1c %></td>
        <td><%= bsdi_blood_chemistry.bsdi_blood_chem_risk %></td>
        <td><%= link_to 'Show', bsdi_blood_chemistry %></td>
        <td><%= link_to 'Edit', edit_bsdi_blood_chemistry_path(bsdi_blood_chemistry) %></td>
        <td><%= link_to 'Destroy', bsdi_blood_chemistry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

