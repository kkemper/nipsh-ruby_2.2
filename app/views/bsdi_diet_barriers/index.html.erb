<h1>Listing Bsdi Diet Barriers</h1>

<table>
  <thead>
    <tr>
      <th>Bsdi report</th>
      <th>Bsdi rfc diet</th>
      <th>Bsdi diet barrier unfamililar</th>
      <th>Bsdi diet barrier time</th>
      <th>Bsdi diet barrier people</th>
      <th>Bsdi diet barrier taste</th>
      <th>Bsdi diet barrier other</th>
      <th>Bsdi diet barrier expense</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_diet_barriers.each do |bsdi_diet_barrier| %>
      <tr>
        <td><%= bsdi_diet_barrier.bsdi_report_id %></td>
        <td><%= bsdi_diet_barrier.bsdi_rfc_diet %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_unfamililar %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_time %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_people %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_taste %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_other %></td>
        <td><%= bsdi_diet_barrier.bsdi_diet_barrier_expense %></td>
        <td><%= link_to 'Show', bsdi_diet_barrier %></td>
        <td><%= link_to 'Edit', edit_bsdi_diet_barrier_path(bsdi_diet_barrier) %></td>
        <td><%= link_to 'Destroy', bsdi_diet_barrier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

