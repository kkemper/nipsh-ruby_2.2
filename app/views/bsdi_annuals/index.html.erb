      <th>Bsdi emergency room</th>
      <th>Bsdi high absenteeism</th>
      <th>Bsdi seat belt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bsdi_annuals.each do |bsdi_annual| %>
      <tr>
        <td><%= bsdi_annual.bsdi_report_id %></td>
        <td><%= bsdi_annual.bsdi_see_doctor %></td>
        <td><%= bsdi_annual.bsdi_see_dentist %></td>
        <td><%= bsdi_annual.bsdi_check_bp %></td>
        <td><%= bsdi_annual.bsdi_check_chol %></td>
        <td><%= bsdi_annual.bsdi_colonoscopy %></td>
        <td><%= bsdi_annual.bsdi_papsmear %></td>
        <td><%= bsdi_annual.bsdi_mammogram %></td>
        <td><%= bsdi_annual.bsdi_prostate %></td>
        <td><%= bsdi_annual.bsdi_breast_exam %></td>
        <td><%= bsdi_annual.bsdi_testicular_exam %></td>
        <td><%= bsdi_annual.bsdi_flu_shot %></td>
        <td><%= bsdi_annual.bsdi_physician_visits %></td>
        <td><%= bsdi_annual.bsdi_hospital_visits %></td>
        <td><%= bsdi_annual.bsdi_emergency_room %></td>
        <td><%= bsdi_annual.bsdi_high_absenteeism %></td>
        <td><%= bsdi_annual.bsdi_seat_belt %></td>
        <td><%= link_to 'Show', bsdi_annual %></td>
        <td><%= link_to 'Edit', edit_bsdi_annual_path(bsdi_annual) %></td>
        <td><%= link_to 'Destroy', bsdi_annual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

